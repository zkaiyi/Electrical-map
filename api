    "LXR": "彭剑",                             ---联系人
    "LXDH": "15906585002",                     ---联系人手机号
    "SSJD": "白云街道",                        ---所属街道
    "QYFL": "电商服务业务",                    ---企业分类
    "QYFR": "邵丽莉",                          ---企业法人
    "QYFRDH": "13957872430",                   ---企业法人手机号
